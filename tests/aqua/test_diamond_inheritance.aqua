class
   Test_Diamond_Inheritance
   
inherit
   Unit_Test
      redefine Name, Execute end
   Aqua.Text_IO
   
feature

   A : Diamond_A
   B : Diamond_B
   C : Diamond_C
   D : Diamond_D

   Value : Integer
      do
         Result := 10
      end
         
   Name : String
      do
         Result := "test-diamond-inheritance"
      end
      
   Execute
      do
         Success := Value = 10
         if not Success then
            Fail_Message := "Expected Value = 10; found " & Value.To_String
         end
      end
       
end 
