themes
{
  default
  {
    fixed_font
    {
      font_family = "Courier New";
      font_size = 10;
    }
    
    classes
    {
    default
    {
    }
    reserved_identifier
    {
      bold = true;
      --  foreground = purple;
    }
    comment
    {
      foreground = blue;
    }
    string
    {
      foreground = rosybrown;
    }
    package_name
    {
      foreground 
      {
        r = 166;
        g = 0;
        b = 217;
      }
    }
    function_name
    {
      foreground 
      {
        r = 166;
        g = 0;
        b = 217;
      }
    }
    variable_name
    {
      foreground = darkgoldenrod;
    }
    type_name
    {
      foreground = forestgreen;
    }
    warning
    {
      underline = true;
    }
    error
    {
      --  foreground = red;
      --  bold = true;
      underline = true;
    }
    selected_error
    {
      foreground = white;
      background = red;
    }
    }
  }
}

