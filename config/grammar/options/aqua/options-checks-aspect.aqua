class Options.Checks.Aspect
   inherit Aqua.Text_IO
           Options.Option_Entry_Node
           
feature

   Node  : Options.Syntax.Aspect_Node
   
   After_Node
   do
      Put_Line ("Options.Checks.Aspect")
      if attached Node.String_Literal as Long_Name then
         Option_Entry.Set_Long_Name (Long_Name)
      end
      if attached Node.Character_Literal as C then
         Option_Entry.Set_Short_Name (C)
      end
   end
   
end