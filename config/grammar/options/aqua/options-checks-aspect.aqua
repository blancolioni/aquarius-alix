class
   Options.Checks.Aspect
  
inherit 
   Options.Option_Entry_Node
   
feature

   After_String_Literal (Text : String)
      do
         Option_Entry.Set_Long_Name (Text)
      end
      
   After_Character_Literal (Text : String)
      do
         Option_Entry.Set_Short_Name (Text)
      end
   
end