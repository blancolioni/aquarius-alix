plugin Aqua version 0.1 is

   source_path = (".", "/", "/standard", "/binary", "/standard/generated");
   
   group run_check : semantic is
      aqua_check;
   end run_check;

   group resolve_references : semantic is
      aqua_resolve;
   end resolve_references;

   group tagatha : code is
      aqua_tagatha;
   end tagatha;
   
end Aqua;