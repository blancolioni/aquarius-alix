@!aquarius:actionscript
@before file_declaration do
@IO.set_output (tree.file_name.to_lower & ".adb");
package body [tree.file_name] is

   ----------
   -- Load --
   ----------
   
   procedure Load 
     (File : in out File_Type;
      Path : String)
   is
      use WL.Binary_IO;
      Bin : File_Type;
   begin
      Open (Bin, In_File, Path);
      Close (Bin);
   end Load;
@end;
@after file_declaration do
end [tree.file_name];
@IO.set_output (0);
@end;
