class Petal.Checks.Top_Level_Interface_Declaration
inherit
   Aqua.Text_IO
   Petal.Declaration
   Petal.Top_Level
   
feature

   Interface : Petal.Type.Interface
      
   After_Qualified_Identifier (Child : Petal.Checks.Qualified_Identifier)
      do
         Put_Line ("interface " & Child.Identifiers.Interpolate ("."))
         create Entry
         create Interface
         Entry.Create_Entry (Child.Identifiers.Last_Element, Void, Interface)
      end
      
end
