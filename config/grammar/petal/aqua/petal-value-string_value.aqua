class Petal.Value.String_Value
inherit Petal.Value redefine To_String end
create Make
   
feature
   Make (Value : String)
     do
        S_Value := Value
     ensure
        Initialized: S_Value = Value
     end

   To_String : String
      require
         Not_Void: S_Value /= Void 
      do
         Result := S_Value
      end
      
feature

   S_Value : String
   
end