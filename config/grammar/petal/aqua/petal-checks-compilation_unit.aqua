class Petal.Checks.Compilation_Unit
inherit
   Petal.Top_Level
   Aqua.Text_IO
   
feature

   Before_Top_Level_Procedure_Declaration (Child : Petal.Checks.Top_Level_Procedure_Declaration)
      local
         String_Lits : Petal.String_Table
      do
         create String_Lits
         Child.Set_String_Table (String_Lits)
         Child.Set_Context (Standard)
         Put_Line ("context: " & Child.Context.Table_Name)
      end
   
end
