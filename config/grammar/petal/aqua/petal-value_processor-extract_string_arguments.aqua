class
   Petal.Value_Processor.Extract_String_Arguments
   
inherit
   Petal.Value_Processor
   
feature

   Index : Integer
   
   Sent_String : Petal.Value
   
   Process (V : Petal.Value)
      do
         Index := Index + 1
         if Index = 3 then
            Sent_String := V
         end
      end
      
end
