class
   Klein.Checks.Inherited_Class_Name
   
inherit
   Klein.With_Table
   
feature

   Name  : String
   Entry : Table_Entry
   Type  : Object_Type
   
   After_Type_Name (Child : Klein.Checks.Type_Name)
      do
		 Name  := Child.Name
		 Entry := Child.Entry
		 Type  := Child.Type
      end

end
