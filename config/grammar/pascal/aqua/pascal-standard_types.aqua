class
   Pascal.Standard_Types
   
feature

   Standard_Boolean : Pascal.Object_Type
      do
	     create {Pascal.Object_Type.Primitive_Boolean}.Make_Boolean (Result)
	  end
	
   Standard_Integer : Pascal.Object_Type
      do
         create {Pascal.Object_Type.Primitive_Integer}.Make_Object (Result)
      end
	  
   Standard_Real : Pascal.Object_Type
      do
         create {Pascal.Object_Type.Primitive_Real}.Make_Object (Result)
      end
	  
end