class Ada.Library.Subprogram_Specification inherit
   Ada.Specification
   
feature

   After_Procedure_Specification (Child : Ada.Library.Procedure_Specification)
      do
         Full_Name := Child.Full_Name
         Local_Name := Child.Local_Name
      end

   After_Function_Specification (Child : Ada.Library.Function_Specification)
      do
         Full_Name := Child.Full_Name
         Local_Name := Child.Local_Name
      end

end
