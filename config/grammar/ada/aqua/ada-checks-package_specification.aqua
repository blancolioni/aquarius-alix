class Ada.Checks.Package_Specification inherit
   Ada.Checks.Declared

feature

   After_Defining_Program_Unit_Name (Child : Defining_Program_Unit_Name)
      do
         Full_Name := Child.Identifiers.Interpolate (".")
         Name := Child.Identifiers.Last_Element
      end
      
   Before_Matching_Name (Child : Matching_Name)
      do
         Child.Set_Original_Name (Full_Name)
      end
end
