class Ada.Entity_Reference
inherit
   Aqua.Text_IO
   
feature
   Name      : String
   Tree      : Aquarius.Trees.Program_Tree
   Entity    : Komnenos.Entity
   
   Make (Ref_Name   : String
         Ref_Entity : Komnenos.Entity
         Ref_Tree   : Aquarius.Trees.Program_Tree)
      do
         Name := Ref_Name
         Tree := Ref_Tree
         Entity := Ref_Entity
         Put_Line ("New reference to " & Ref_Name & " at " & Ref_Tree.Concatenated_Image)
      end
end