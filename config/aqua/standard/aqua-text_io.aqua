class Aqua.Text_IO

feature

   Put (S : String) external "aqua"
   
   Put_Line (S : String)
      do
         Put (S)
         New_Line
      end
      
   New_Line external "aqua" alias "io.new_line"
   Set_Output (Path : String) external "aqua" alias "io.set_output"
   Set_Col (Col : Integer) external "aqua" alias "io.set_col"
   Col : Integer external "aqua" alias "io.col"