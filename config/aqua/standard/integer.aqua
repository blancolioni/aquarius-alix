expanded class 
   Integer
   
inherit  
   Aqua.Algebra.Additive redefine Plus end
   Aqua.Algebra.Eq redefine Equal end
  
feature
   To_String : String external "aqua"
   
   Plus alias "+" (Right : Integer) : Integer external "intrinsic"
   Equal alias "=" (Right : Integer) : Boolean external "intrinsic"

   GE alias ">=" (Right : Integer) : Boolean
   LE alias "<=" (Right : Integer) : Boolean
   GT alias ">" (Right : Integer) : Boolean
   LT alias "<" (Right : Integer) : Boolean
   
feature { None }

   Internal : System.Word_32
   
end
